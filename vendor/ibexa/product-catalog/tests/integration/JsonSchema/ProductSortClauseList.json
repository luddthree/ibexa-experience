            "type": "object",
            "properties": {
                "_media-type": {
                    "type": "string"
                },
                "ProductSortClause": {
                    "type": "array",
                    "items": [
                        {
                            "type": "object",
                            "properties": {
                                "_media-type": {
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "_media-type",
                                "name"
                            ]
                        }
                    ]
                }
            },
            "required": [
                "_media-type",
                "ProductSortClause"
            ]
        }
    },
    "required": [
        "ProductSortClauseList"
    ]
}
