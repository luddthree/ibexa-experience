{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "CurrencyList": {
            "type": "object",
            "properties": {
                "_media-type": {
                    "type": "string"
                },
                "Currency": {
                    "type": "array",
                    "items": [
                        {
                            "type": "object",
                            "properties": {
                                "_media-type": {
                                    "type": "string"
                                },
                                "id": {
                                    "type": "integer"
                                },
                                "code": {
                                    "type": "string"
                                },
                                "subunits": {
                                    "type": "integer"
                                },
                                "enabled": {
                                    "type": "boolean"
                                }
                            },
                            "required": [
                                "_media-type",
                                "id",
                                "code",
                                "subunits",
                                "enabled"
                            ]
                        }
                    ]
                }
            },
            "required": [
                "_media-type",
                "Currency"
            ]
        }
    },
    "required": [
        "CurrencyList"
    ]
}
